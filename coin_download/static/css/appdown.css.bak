.browserOpenBox {
  position: fixed;
  display: none;
  z-index: 9;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(61, 61, 108, .5);
}

.browserOpen {
  position: absolute;
  z-index: 9;
  right: 2px;
  top: 13px;
  letter-spacing: 1px;
  color: #fff;
  background-color: #4b4a7c;
  border-radius: 6px;
  font-size: 12px;
  line-height: 22px;
  padding: 12px;
  cursor: pointer;
}

.browserOpen::before {
  position: absolute;
  content: '';
  border-bottom: 9px solid #4b4a7c;
  border-left: 9px solid transparent;
  border-right: 9px solid transparent;
  top: -9px;
  right: 18px;
}


/* ios教程 */
.ios-testflight,
